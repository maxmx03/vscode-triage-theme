    "gitDecoration.conflictingResourceForeground": "#9966ff",
    "gitDecoration.deletedResourceForeground": "#e0115f",
    "gitDecoration.modifiedResourceForeground": "#FFD700",
    "gitDecoration.stageModifiedResourceForeground": "#cd7f32"
